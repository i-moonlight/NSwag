<#      }#>
<#  }#>
<#  if(operation.DefaultResponse.IsSuccess){#>
                return result; 
<#  }else{#>
                throw result; 
<#  }
}else{#>
                throw new Error("error_no_callback_for_the_received_http_status"); 
<#}#>
            }
        });
    }
<#}#>}
