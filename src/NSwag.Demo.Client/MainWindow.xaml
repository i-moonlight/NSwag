<Window x:Class="NSwag.Demo.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Margin="12">
        <TextBlock Text="A" FontWeight="Bold" />
        <TextBox x:Name="A" Margin="0,0,0,12" />

        <TextBlock Text="B" FontWeight="Bold" />
        <TextBox x:Name="B" Margin="0,0,0,12" />

        <TextBlock Text="C" FontWeight="Bold" />
        <TextBox x:Name="C" Margin="0,0,0,12" />
        
        <Button Content="Calculate" Click="OnCalculate" Margin="0,0,0,12" />

        <TextBlock Text="Result" FontWeight="Bold" />
        <TextBox x:Name="Result" />
    </StackPanel>
</Window>
