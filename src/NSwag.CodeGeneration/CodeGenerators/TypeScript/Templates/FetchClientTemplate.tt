                <#=ConversionUtilities.Tab(response.DataConversionCode, 4)#>
<#      }else{#>
                result<#=response.StatusCode#> = data === "" ? null : <<#=response.Type#>>JSON.parse(data);
<#      }#>
<#  }#>
<#  if(response.IsSuccess){#>
                return result<#=response.StatusCode#>; 
<#  }else{#>
