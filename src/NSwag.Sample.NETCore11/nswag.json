{
  "runtime": "NetCore11",
  "swaggerGenerator": {
    "webApiToSwagger": {
      "controllerNames": [
        "NSwag.Sample.NETCore11.Controllers.PetController"
      ],
      "isAspNetCore": true,
      "defaultUrlTemplate": "api/{controller}/{id?}",
      "defaultPropertyNameHandling": "Default",
      "defaultReferenceTypeNullHandling": "Null",
