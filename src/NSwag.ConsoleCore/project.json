{
  "version": "8.8.0-*",
  "authors": [ "Rico Suter" ],
  "description": "NSwag: The Swagger API toolchain for .NET and TypeScript",
  "packOptions": {
    "projectUrl": "https://github.com/NSwag/NSwag",
    "licenseUrl": "https://github.com/NSwag/NSwag/blob/master/LICENSE.md",
    "iconUrl": "https://raw.githubusercontent.com/NSwag/NSwag/master/assets/NuGetIcon.png",
    "tags": [ "Swagger", "Documentation", "WebApi", "AspNet", "TypeScript", "CodeGen" ]
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-nswag"
  },

  "dependencies": {
    "System.IO.FileSystem": "4.3.0",
    "System.Net.Http": "4.3.0",
    "NSwag.AssemblyLoaderCore": "*",
    "NSwag.Commands": "*",
    "NSwag.Core": "*",
    "NSwag.Annotations": "*",
    "NSwag.CodeGeneration": "*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "netcoreapp1.1": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      }
    }
  },

  "runtimeOptions": {
    "applyPatches": true
  }
}